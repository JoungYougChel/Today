    <!-- dialog -->
    <style name="MaterialAlertDialog" parent="ThemeOverlay.MaterialComponents.MaterialAlertDialog">
        <item name="android:textSize">15dp</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textColorPrimary">@android:color/black</item>
        <item name="colorPrimary">@color/Main</item>
        <item name="windowMinWidthMajor">80%</item>
        <item name="windowMinWidthMinor">80%</item>
        <item name="cornerRadius">20dp</item>
    </style>

    <style name="CustomDialog" parent="Theme.AppCompat.Dialog">
        <item name="windowNoTitle">true</item>
        <item name="windowMinWidthMajor">90%</item>
        <item name="windowMinWidthMinor">90%</item>
        <item name="cornerRadius">20dp</item>
    </style>

    <!-- Bottom Nav   -->
    <style name="BottomNavigationView">
        <item name="itemTextAppearanceActive">@style/TextAppearance.BottomNavigationView.Active</item>
        <item name="itemTextAppearanceInactive">@style/TextAppearance.BottomNavigationView.Inactive</item>
    </style>

    <!-- blank styles for better code readability-->
    <style name="TextAppearance"/>
    <style name="TextAppearance.BottomNavigationView"/>

    <!-- inactive tab icon style -->
    <style name="TextAppearance.BottomNavigationView.Inactive">
        <item name="android:textSize">11dp</item>
        <item name="fontFamily">@font/pretendard_medium</item>
    </style>

    <!-- active tab icon style -->
    <style name="TextAppearance.BottomNavigationView.Active">
        <item name="android:textSize">11dp</item>
    </style>

    <style name="CustomBottomSheetDialogTheme" parent="Theme.MaterialComponents.Light.BottomSheetDialog">
