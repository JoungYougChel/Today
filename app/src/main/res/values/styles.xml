<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:colorPrimary">@color/Bg</item>
        <item name="android:colorPrimaryDark">@color/colorBlack</item>
        <item name="android:colorAccent">@color/Main</item>
        <item name="android:windowBackground">@color/Bg</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">@color/Bg</item>
        <item name="android:textCursorDrawable">@drawable/cursor_main</item>
        <item name="fontFamily">@font/pretendard_regular</item>
        <item name="android:textColor">@color/Base0</item>
        <item name="android:textSize">15sp</item>
    </style>

